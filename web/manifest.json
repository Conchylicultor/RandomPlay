{
  "name": "RandomPlay",
  "description": "Select a video and play it in a new tab",
  "version": "0.6.0",
  
  "manifest_version": 2,
  
  "permissions": [
    "bookmarks"
  ],
  
  "icons": {"128": "play.png"},

  "background": {
    "scripts": ["main.dart.js"]
  },

  "browser_action": {
      "default_icon" : "play.png",
      "default_title": "Play a song"
  }
}
